{
  "name": "ai-image-order-reconciliation",
  "version": "1.0.0",
  "description": "Custom T-Shirt E-commerce Application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "test": "cd client && npm test",
    "test:watch": "cd client && npm test -- --watch",
    "test:coverage": "cd client && npm test -- --coverage",
    "test:components": "cd client && npm test -- --testPathPattern=components",
    "test:integration": "cd client && npm test -- --testPathPattern=integration"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "form-data": "^4.0.4",
    "multer": "^1.4.5-lts.1",
    "pdfkit": "^0.14.0",
    "sharp": "^0.34.3",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "e-commerce",
    "t-shirt",
    "custom",
    "orders"
  ],
  "author": "Your Name",
  "license": "MIT"
}
